package com.catalogy.moviectalogservice;

public class Movie {
	
	private int id;
	private String name;
	private String description;
	private int rating;
	
	public Movie() {
		super();
	}

	public Movie(int id, String name, String description, int rating) {
		super();
		this.id = id;
		this.name = name;
		this.description = description;
		this.rating = rating;
	}
	
	
}
